}
.page0 .slogan>p:nth-child(4){
    background: url("../img/p0-p4.png") no-repeat center;
    background-size: contain;
    height: 0.2rem;
    margin-top: 0.16rem;
}
.page0.show .slogan>p:nth-child(1){
    -webkit-animation:fadein .3s .6s linear both;
}
.page0.show .slogan>p:nth-child(2){
    -webkit-animation:fadein .3s .9s linear both;
}
.page0.show .slogan>p:nth-child(3){
    -webkit-animation:fadein .3s 1.2s linear both;
}
.page0.show .slogan>p:nth-child(4){
    -webkit-animation:fadein .3s 1.5s linear both;
}
@-webkit-keyframes fadein {
    0%{
        opacity:0;
    }
    100%{
        opacity:1
    }
}
.page0 .video0{
    position: relative;
    width: 100%;
    height: 3.6rem;
    background: url("../img/p0-poster.jpg") no-repeat center;
    background-size: cover;
    margin-top: 0.4rem;
}
.page0 .video0>canvas{
    width: 100%;
    height: 100%;
}
.page0 .video0>.playbtn{
    background: url("../img/playbtn.png") no-repeat center;
    background-size: contain;
    width: 1.79rem;
    height: 1.79rem;
    position: absolute;
    top:50%;
    left: 50%;
    -webkit-transform: translate(-50%,-50%);
    z-index: 1;
}
.page0 .arrow{
    position: absolute;
    top:10rem;
    background: url("../img/p0-arrow.png") no-repeat center;
    background-size: contain;
    width: 100%;
    height: 0.25rem;
    z-index: 1;
}
.page0.show .arrow{
    -webkit-animation:fadeup 1s 1s infinite linear both;
}
@-webkit-keyframes fadeup {
    0%{
        -webkit-transform: translate(0,0);
        opacity: 1;
    }
    20%{
        -webkit-transform: translate(0,0);
        opacity: 1;
    }
    90%{
        -webkit-transform: translate(0,-200%);
        opacity: 0;
    }
    100%{
        -webkit-transform: translate(0,0);
        opacity: 0;
    }
}
.page0 .moreinfo{
    position: absolute;
    bottom: 0.2rem;
    left: 50%;
    -webkit-transform: translate(-50%,0);
    background: url("../img/p0-moreinfo.png") no-repeat center;
    background-size: contain;
    width: 1.59rem;
    height: 0.35rem;
}
