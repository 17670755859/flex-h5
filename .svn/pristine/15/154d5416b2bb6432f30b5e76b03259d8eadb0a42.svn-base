* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    -webkit-tap-highlight-color: transparent;
}
body {
    font-family: -apple-system, BlinkMacSystemFont, "PingFang SC", "Helvetica Neue", STHeiti, "Microsoft Yahei", Tahoma, Simsun, sans-serif;
    font-size: 0;
}
li {
    list-style: none;
}
input {
    -webkit-appearance: none;
    outline: none;
    border-radius: 0;
    border: 0;
}
input:focus {
    outline: 0;
}

textarea {
    -webkit-appearance: none;
    outline: none;
    border-radius: 0;
}
/*************************************/
html,body{
    width: 100%;
    max-width: 640px;
    height: 100%;
    margin: auto;
}
body>div{
    position: absolute;
    top:0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    z-index: 1;
    -webkit-transition: all .3s;
}
body>div.show{
    opacity: 1;
    z-index: 11;
}
/*************************************/
.page0{
