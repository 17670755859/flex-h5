.page0 .slogan{
    position: relative;
    z-index: 1;
    width: 100%;
    margin-top: 0.25rem;
}
.page0 .slogan>p:nth-child(1){
    background: url("../img/p0-p1.png") no-repeat center;
    background-size: contain;
    height: 0.24rem;
}
.page0 .slogan>p:nth-child(2){
    background: url("../img/p0-p2.png") no-repeat center;
    background-size: contain;
    height: 0.2rem;
    margin-top: 0.22rem;
}
.page0 .slogan>p:nth-child(3){
